{
  "extends": [
    "tslint:recommended",
    "tslint-config-airbnb"
  ],
  "rules": {
    "semicolon": [
      true,
      "never"
    ],
    "quotemark": [
      false
    ],
    "object-literal-sort-keys": false,
    "import-name": false,
    "prefer-array-literal": [
      true,
      {
        "allow-type-parameters": true
      }
    ],
    "variable-name": [
      false
    ],
    "trailing-comma": [
      true,
      {
        "singleline": {
          "functions": "never",
          "arrays": "never"
        },
        "multiline": {
          "functions": "never",
          "arrays": "always"
        }
      }
    ]
  }
}
